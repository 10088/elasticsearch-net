@using System.Collections
@using System.Collections.Generic
@using System.Net.Configuration
@using CodeGeneration.YamlTestsRunner.Domain
@using CodeGeneration.YamlTestsRunner;
@{
	IEnumerable<ITestStep> m = Model;
}
@foreach (var step in m)
{
	switch (step.Type)
	{
		case "do": @RenderPage("Do.cshtml", (DoStep) step)
			break;
		case "set": @RenderPage("Set.cshtml", (SetStep) step)
			break;
		case "is_true": @RenderPage("IsTrue.cshtml", (IsTrueStep) step)
			break;
		case "is_false": @RenderPage("IsFalse.cshtml", (IsFalseStep) step)
			break;
		case "lt": @RenderPage("LowerThan.cshtml", (LowerThanStep) step)
			break;
		case "gt": @RenderPage("GreaterThan.cshtml", (GreaterThanStep) step)
			break;
		case "length": @RenderPage("Length.cshtml", (LengthStep) step)
			break;
	}
}