@using System.Net.Configuration
@using CodeGeneration.YamlTestsRunner.Domain
@using CodeGeneration.YamlTestsRunner;
@{
	YamlDefinition m = Model;
	var className = m.Folder.ToPascalCase() + m.FileName.ToPascalCase() + "Base";
	
}
@if (m.SetupSuite != null)
{
	var suite = m.SetupSuite;
	<text>
		public class @(className) : YamlTestsBase
		{
			public @(className)() : base()
			{	@RenderPage("DispatchSteps.cshtml", suite.Steps)
			}
		}</text>
}